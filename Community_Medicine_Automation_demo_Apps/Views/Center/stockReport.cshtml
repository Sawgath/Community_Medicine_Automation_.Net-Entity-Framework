@{
    ViewBag.Title = "stockReport";
    Layout = "~/Views/Shared/_Layout2.cshtml";
}


<div class="center-block">
    <div class="row">
        <div class="col-lg-3 col-md-3"></div>
        <div class="col-lg-6 col-md-6 text-center postTopic1">
            <br/>
            <h2>Stock Report</h2>
            <br/>
            <table id="Table1" class="table table-striped">
                <thead>
                <tr>
                    <td>Medicine ID</td>
                    <td>
                        Medicine Name
                    </td>

                    <td>
                        Quantity
                    </td>
                </tr>
                </thead>
                <tbody id="tbody1">

                @foreach (var department in ViewBag.as1)
                {
                    <tr>
                        <td>
                            @department.centerID
                        </td>
                        <td>
                            @department.medicineName
                        </td>

                        <td>
                            @department.stock
                        </td>
                    </tr>


                }

                </tbody>
            </table>


            <br>
            <br>
            <br>

        </div>
        <div class="col-lg-3 col-md-3"></div>
    </div>

</div>