@model Community_Medicine_Automation_demo_Apps.Models.ListingPair2
@if (Model.ACenterTbs != null && Model.ACenterTbs.Count() > 0)
{
    using (Ajax.BeginForm("Medicine_Supply2", "HeadOffice", new AjaxOptions { UpdateTargetId = "Medicine_Supply3" }))
    {
        @Html.HiddenFor(m => m.ACenterTbs)
        @Html.DropDownListFor(
            m => m.ACenterTbs,
            new SelectList(Model.ACenterTbs, "centerID", "thana"),
            string.Empty
            )
    }
}
<script type="text/javascript">
    $('#ACenterTbs').change(function () {
        $(this).parents('form').submit();
    });
</script>